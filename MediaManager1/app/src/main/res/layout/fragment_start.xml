<?xml version="1.0" encoding="utf-8"?>

<layout>
    
    <data>
        
        <variable
            name="listener"
            type="com.t3h.mediamanager1.interfaceFragment.ClickFmStart" />
        
    </data>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:background="#0883E4"></LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:background="#DBDBDB">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="#0883E4"></LinearLayout>

            <androidx.cardview.widget.CardView
                android:layout_centerVertical="true"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginRight="20dp"
                android:background="#FFFFFF">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="5">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:onClick="@{(v)->listener.onClickFmMusic()}">

                            <ImageView
                                android:id="@+id/im3"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:src="@drawable/music"
                                android:layout_centerVertical="true"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Music"
                                android:textSize="15dp"
                                android:textColor="#000000"
                                android:layout_centerHorizontal="true"
                                android:layout_below="@+id/im3"/>

                        </RelativeLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#DBDBDB"></View>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="6"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:onClick="@{(v)->listener.onClickFmVideo()}">

                                <ImageView
                                    android:id="@+id/im4"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:src="@drawable/video"
                                    android:layout_centerVertical="true"
                                    android:layout_centerHorizontal="true"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Video"
                                    android:textSize="15dp"
                                    android:textColor="#000000"
                                    android:layout_centerHorizontal="true"
                                    android:layout_below="@+id/im4"/>

                            </RelativeLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#DBDBDB"></View>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:onClick="@{(v)->listener.onClickFmImage()}">

                                <ImageView
                                    android:id="@+id/im6"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:src="@drawable/image"
                                    android:layout_centerVertical="true"
                                    android:layout_centerHorizontal="true"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Image"
                                    android:textSize="15dp"
                                    android:textColor="#000000"
                                    android:layout_centerHorizontal="true"
                                    android:layout_below="@+id/im6"/>

                            </RelativeLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="5">

            <TextView
                android:padding="10dp"
                android:id="@+id/tv_tv1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Your Folder"
                android:textSize="15dp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignTop="@+id/tv_tv1"
                android:layout_marginTop="0dp"
                android:background="#DBDBDB"></View>

            <androidx.cardview.widget.CardView
                android:layout_below="@+id/tv_tv1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginRight="20dp"
                android:background="#FFFFFF">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="6"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:onClick="@{(v) -> listener.onFmYourImg()}">

                            <ImageView
                                android:id="@+id/im9"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:src="@drawable/image"
                                android:layout_centerVertical="true"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Image"
                                android:textSize="15dp"
                                android:textColor="#000000"
                                android:layout_centerHorizontal="true"
                                android:layout_below="@+id/im9"/>

                        </RelativeLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#DBDBDB"></View>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:onClick="@{(v) -> listener.onFmYourVideo()}">

                            <ImageView
                                android:id="@+id/im10"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:src="@drawable/video"
                                android:layout_centerVertical="true"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Video"
                                android:textSize="15dp"
                                android:textColor="#000000"
                                android:layout_centerHorizontal="true"
                                android:layout_below="@+id/im10"/>

                        </RelativeLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#DBDBDB"></View>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="5"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:onClick="@{(v) -> listener.onBackup()}">

                                <ImageView
                                    android:id="@+id/im8"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:src="@drawable/backup"
                                    android:layout_centerVertical="true"
                                    android:layout_centerHorizontal="true"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Backup"
                                    android:textSize="15dp"
                                    android:textColor="#000000"
                                    android:layout_centerHorizontal="true"
                                    android:layout_below="@+id/im8"/>

                            </RelativeLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#DBDBDB"></View>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:onClick="@{(v) -> listener.onDownload()}">

                                <ImageView
                                    android:id="@+id/im7"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:src="@drawable/download"
                                    android:layout_centerVertical="true"
                                    android:layout_centerHorizontal="true"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Download"
                                    android:textSize="15dp"
                                    android:textColor="#000000"
                                    android:layout_centerHorizontal="true"
                                    android:layout_below="@+id/im7"/>

                            </RelativeLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>

        </RelativeLayout>


    </LinearLayout>

</layout>

